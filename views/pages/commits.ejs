<head>
  <%- include('../partials/head'); %>
</head>
<header>
  <%- include('../partials/header'); %>
</header>
<div class="container">
  <div class="row">
    <div class="col card p-0">
      <table class="table table-striped table-bordered text-center mb-0">
        <thead class="table-dark">
          <th>Sr. No.</th>
          <th>Message</th>
          <th>Commited By</th>
          <th>Date</th>
          <th>Action</th>
        </thead>
        <% for(var i=0; i < respCommit.length; i++) { %>
          <% if(respCommit[i].commit?.message.toString().includes('UL-TMP')){ %>
            <tr>
              <td>
                <%= i + 1 %>
              </td>
              <td>
                <%= respCommit[i].commit?.message %>
              </td>
              <td>
                <%= respCommit[i].commit?.committer?.name %>
              </td>
              <td>
                <%= respCommit[i].commit?.committer?.date %>
              </td>
              <td>
                <a href="./files?sha=<%= respCommit[i].sha %>&commit=<%= respCommit[i].commit?.message %>"
                  class="link-primary">View Files</a>
              </td>
            </tr>
            <%}%>
              <%}%>
      </table>
    </div>
  </div>
</div>
  <%- include('../partials/footer'); %>
